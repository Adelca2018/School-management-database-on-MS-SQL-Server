/****** Object:  View [dbo].[Notendurchschnitt_pro_Lehrer_pro_Schulfach]    Script Date: 30/04/2020 10:50:20 ******/
SET ANSI_NULLS ON
GO
SET QUOTED_IDENTIFIER ON
GO

-----------------------------------------------------------------
--diese Ansicht zeigt den Notendurchschnitt der Schüler in dem jeweiligen Fach, welche ein Lehrer unterrichtet
--diese Abfrage hat Potential für eine Tabellenfunktion
-----------------------------------------------------------------

CREATE VIEW [dbo].[Notendurchschnitt_pro_Lehrer_pro_Schulfach] AS
SELECT 
dbo.tb_Klassen.Klassen_Stufe, 
dbo.tb_Klassen.Klassen_Untergruppe,
dbo.tb_Schulfächer.Schulfach_Bezeichnung, 
dbo.tb_Schüler.Schüler_Name, 
dbo.tb_Schüler.Schüler_Vorname,
dbo.tb_Ergebnisse.Schüler_ID,
AVG(dbo.tb_Ergebnisse.Note) AS Notendurchnitt
FROM dbo.tb_Ergebnisse 
INNER JOIN dbo.tb_Schulfächer ON dbo.tb_Ergebnisse.Schulfach_ID = dbo.tb_Schulfächer.Schulfach_ID 
INNER JOIN dbo.tb_Schüler ON tb_Ergebnisse.Schüler_ID = dbo.tb_Schüler.Schüler_ID  
INNER JOIN dbo.tb_Klassen ON dbo.tb_Ergebnisse.Klassen_ID = dbo.tb_Klassen.Klassen_ID 
INNER JOIN dbo.tb_Verbindung_Lehrer_Klasse_Schuljahr ON tb_Verbindung_Lehrer_Klasse_Schuljahr.Klassen_ID=dbo.tb_Klassen.Klassen_ID
WHERE tb_Verbindung_Lehrer_Klasse_Schuljahr.Mitarbeiter_ID = 8
GROUP BY dbo.tb_Klassen.Klassen_Stufe, 
dbo.tb_Klassen.Klassen_Untergruppe, 
